      .navigation-hamburger {
        display: none;
        vertical-align: middle;
        position: relative;
        width: 56px;
        height: 56px;
        cursor: pointer;
        color: #000
      }

      .navigation-hamburger .bar-wrap {
        position: absolute;
        top: 50%;
        left: 50%;
        overflow: hidden;
        width: 20px;
        z-index: 999999;
        filter: blur(0);
        -webkit-font-smoothing: subpixel-antialiased;
        transform-style: preserve-3d;
        transform: translate(-50%) translateY(-50%) translateZ(0);
        transition: margin .2s ease, transform .2s ease .8s, opacity .3s ease
      }

      .navigation-hamburger .bar {
        height: 2px;
        background: #000;
        display: block;
        margin: 4px auto;
        border-radius: 5px;
        transition: all .3s ease 0s;
        background-color: var(--white-color)
      }

      .navigation-hamburger.-close .bar,
      .navigation-hamburger.-close .cross-line {
        animation-duration: .5s;
        animation-fill-mode: forwards
      }

      .navigation-hamburger.-close .bar1 {
        transform: translate(-25px);
        animation-name: jiggleClose;
        animation-delay: .5s
      }

      .navigation-hamburger.-close .bar2 {
        transform: translate(25px);
        animation-name: jiggleClose;
        animation-delay: .65s
      }

      .navigation-hamburger.-close .bar3 {
        transform: translate(-25px);
        animation-name: jiggleClose;
        animation-delay: .8s
      }

      .navigation-hamburger .cross-wrap {
        position: absolute;
        top: 50%;
        left: 50%;
        width: 24px;
        height: 24px;
        overflow: hidden;
        margin-left: 2px;
        filter: blur(0);
        -webkit-font-smoothing: subpixel-antialiased;
        transform-style: preserve-3d;
        transform: translate(-50%) translateY(-50%) translateZ(0);
        transition: all .3s ease
      }

      .navigation-hamburger .cross-wrap .cross-line {
        height: 100%;
        width: 2px;
        background: var(--white-color);
        position: absolute;
        left: 9px;
        border-radius: 5px
      }

      .navigation-hamburger .cross-wrap .cross-line1 {
        transform: translateY(-30px) translate(30px) rotate(45deg)
      }

      .navigation-hamburger.-close .cross-line1 {
        transform: translateY(0) translate(0) rotate(-45deg);
        animation-name: crossLeftClose;
        animation-delay: .15s
      }

      .navigation-hamburger .cross-wrap .cross-line2 {
        transform: translateY(-30px) translate(-30px) rotate(-45deg)
      }

      .navigation-hamburger.-close .cross-line2 {
        transform: translateY(0) translate(0) rotate(45deg);
        animation-name: crossRightClose;
        animation-delay: .3s
      }

      @keyframes jiggleClose {
        0% {
          transform: translate(-25px)
        }

        50% {
          transform: translate(10px)
        }

        to {
          transform: translate(0)
        }
      }

      @keyframes crossLeftClose {
        to {
          transform: translateY(30px) translate(30px) rotate(-45deg)
        }
      }

      @keyframes crossRightClose {
        to {
          transform: translateY(30px) translate(-30px) rotate(45deg)
        }
      }

